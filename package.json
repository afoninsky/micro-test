{
  "name": "micro-test",
  "version": "1.0.1",
  "description": "test",
  "main": "index.js",
  "scripts": {
    "start": "node bin/service",
    "test": "eslint src && nyc ava && nyc check-coverage --lines 90 --functions 80 --branches 60",
    "open:coverage": "nyc report --reporter=html && open coverage/index.html",
    "dep:update": "npm-check -u"
  },
  "author": "Afoninsky / vkfont@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "ava": "*",
    "axios": "*",
    "babel-eslint": "*",
    "eslint": "*",
    "nyc": "*"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "bluebird": "^3.4.1",
    "bunyan": "^1.8.1",
    "bunyan-logstash": "^0.3.4",
    "chalk": "^1.1.3",
    "config": "^1.21.0",
    "koa": "^2.0.0-alpha.6",
    "lodash": "^4.14.1",
    "opbeat": "^3.15.0",
    "seneca": "^3.1.0"
  },
  "peerDependencies": {
    "npm-check": "*"
  },
  "ava": {
    "verbose": true,
    "source": [
      "units/*js"
    ],
    "require": [
      "./test/_preload.js"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ]
  }
}
